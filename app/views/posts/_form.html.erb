<h3>Add Song</h3>
<%= form_for @post do |f| %>
  <% if @post.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= "#{pluralize(@post.errors.count, "error")} prohibited this post from being saved:" %>
      </h2>
      <ul>
        <% @post.errors.full_messages.each do |msg| %>
          <li>
            <%= msg %>
          </li>
          <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :artist %>
    <%= f.text_field :artist %>
  </div>

  <div class="form-group">
    <%= f.label :song %>
    <%= f.text_field :song %>
  </div>

  <div class="form-group">
    <%= f.label :songid %>
    <%= f.text_field :songid %>
  </div>

  <div class="form-group">
    <%= f.button :submit %>
  </div>
<% end %>
